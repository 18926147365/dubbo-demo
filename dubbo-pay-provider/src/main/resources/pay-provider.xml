<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://code.alibabatech.com/schema/dubbo
            http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <!-- 提供方应用信息，用于计算依赖关系 -->
    <dubbo:application name="${dubbo.application.name}" />

    <!-- 使用zookeeper注册中心暴露服务地址 -->
    <dubbo:registry protocol="zookeeper"     address="${dubbo.zookeeper.address}" timeout="${dubbo.zookeeper.timeout}" />


    <!--定义服务提供者-->
    <dubbo:provider  group ="${dubbo.group.name}"  listener="ls"   version="${dubbo.group.version}"/>

    <!--定义服务消费者-->
    <dubbo:consumer  group ="${dubbo.group.name}"   version="${dubbo.group.version}"/>


    <!--<dubbo:service interface="com.omen.service.PayService" version="${dubbo.group.version}"  class="com.omen.service.impl.PayServiceImpl" >-->
        <!--<dubbo:method name="pay2"  cache="myCache"  ></dubbo:method>-->
    <!--</dubbo:service>-->



</beans>
